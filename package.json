        "faker": "^5.3.1",
        "jest": "^26.6.3",
        "mongodb-memory-server": "^6.9.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.0",
        "ts-node-dev": "^1.1.1",
        "tslint": "^5.12.1",
        "typescript": "^3.3.3"
